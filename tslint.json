{
    "extends": ["tslint:latest", "tslint:recommended", "typestrict", "tslint-no-unused-expression-chai"],
    "rules": {
        /*Override stuff in latest*/
        "quotemark": false,
        "object-literal-key-quotes": false,
        "ordered-imports": false,
        "one-variable-per-declaration": false,
        "eofline": false,
        "prefer-for-of": false,
        "jsdoc-format": false,
        "one-line": [true, "check-finally", "check-open-brace"],
        "whitespace": [true, "check-decl", "check-operator", "check-type", "check-typecast"],
        "max-classes-per-file": false,
        "semicolon": false,
        "no-console": false,
        "no-implicit-dependencies": false,
        "trailing-comma": false,
        "no-unnecessary-type-assertion": true,
        "no-floating-promises": true,
        "no-inferred-empty-object-type": true,
        "comment-format": false,
        "no-submodule-imports": false,
        "no-object-literal-type-assertion": false,
        "member-access": false,
        "interface-name": false,
        "max-line-length": false,
        "object-literal-sort-keys": false,
        "member-ordering": false,
        "switch-default": false,
        "variable-name": [true, "ban-keywords", "check-format", "allow-leading-underscore", "allow-pascal-case"],
        "radix": false,
        "arrow-return-shorthand": false,
        "no-namespace": [true, "allow-declarations"],
        "no-var-keyword": true,
        "ban": true,
        "cyclomatic-complexity": [true, 10]
    }
}
